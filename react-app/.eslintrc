{
  "extends": [
    "react-app",
    "airbnb",
    "prettier",
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "semi": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "prettier/prettier": [
      "error", {
        "semi": false
      }
    ],
    "prefer-destructuring": 1
  }
}